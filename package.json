    "@babel/preset-typescript": "^7.3.3",
    "@types/mri": "^1.1.0",
    "@types/pretty-ms": "^4.0.0",
    "@types/string-width": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.9.3",
    "rollup-plugin-add-shebang": "^0.1.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "mri": "^1.1.4",
    "pretty-ms": "^4.0.0",
    "string-width": "^4.0.0"
